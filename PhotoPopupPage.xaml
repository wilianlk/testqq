<?xml version="1.0" encoding="utf-8" ?>
<pages:Popup xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:CommunityToolkit.Maui.Views;assembly=CommunityToolkit.Maui"
             x:Class="Exportacion.PhotoPopupPage">

    <VerticalStackLayout Padding="20" HorizontalOptions="Center" VerticalOptions="Center">
        <Image x:Name="PopupImage"/>
        <Button Text="Close" Clicked="OnCloseClicked"/>
    </VerticalStackLayout>
</pages:Popup>
